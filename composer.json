{
  "name": "vantoozz/lumen-social-app",
  "require": {
    "php": ">=5.6.0",
    "laravel/lumen-framework": "~5",
    "2nova/vk-php-sdk": "~0",
    "vlucas/phpdotenv": "~1",
    "predis/predis": "~1",
    "illuminate/redis": "~5",
    "league/flysystem": "~1",
    "guzzlehttp/guzzle": "~6",
    "filp/whoops": "~1"
  },
  "require-dev": {
    "phploc/phploc": "~2",
    "phpunit/phpunit": "~5",
    "squizlabs/php_codesniffer": "~2",
    "sebastian/phpcpd": "~2",
    "phpmd/phpmd": "~2",
    "codacy/coverage": "~1",
    "satooshi/php-coveralls": "~0",
    "block8/php-docblock-checker": "~1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "classmap": [
      "database/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
